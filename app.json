{
  "expo": {
    "name": "cv-online",
    "slug": "cv-online",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cv-online.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cv-online.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trauss_kk.cvonline"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cv-online.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "db2c82aa-8a29-4c05-9494-44c2c7f18490"
      }
    }
  }
}